@model IEnumerable<Sprint0.Models.SomeModel>
@{
                /**/

                ViewBag.Title = "AllDetails";
}

<h2>AllDetails</h2>


<div class="well">
    <table class="table table-hover table-striped">
        <tr>
            <th>
                Order No
            </th>
            <th>
                Product Name
            </th>
            <th>
                Quantity
            </th>
            <th>
                Unit Price
            </th>
            <th>
                Total Price
            </th>
            <th>
                Order Date
            </th>
            <th>
                Supplier
            </th>
            <th></th>
            @*<th>@Html.DisplayNameFor(model => model.OrderProg)</th>*@
        </tr>

        @foreach (var item in Model)
        {
        <tr>

            <td>
                @item.pOrder.OrderNo

            </td>
            <td>
                @item.orderDetail.ItemName

            </td>
            <td>
                @item.orderDetail.Quantity
            </td>
            <td>
                @item.orderDetail.Rate
            </td>
            <td>
                @item.orderDetail.TotalAmount
            </td>

            <td>
                @item.pOrder.OrderDate
            </td>
                <td>
            @item.pOrder.Description
            </td>
            <td></td>
        </tr>
        }





    </table>
    
</div>
